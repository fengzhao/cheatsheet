{
  "name": "docute",
  "version": "4.11.0",
  "scripts": {
    "serve": "set NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service serve",
    "build:umd": "poi --config build/poi.lib.config.js --prod",
    "build:es": "vue-compile src --config build/vue-compile.config.js",
    "build:vue": "vue-cli-service build",
    "build": "npm run build:vue && npm run build:umd && npm run build:es",
    "test": "npm run lint && npm run test:unit",
    "lint": "xo",
    "website": "poi --serve --config build/poi.website.config.js",
    "build:website": "poi --prod --config build/poi.website.config.js",
    "now-build": "npm run build:website",
    "prepublishOnly": "npm run build",
    "commit": "git-cz",
    "test:unit": "poi puppet src/**/*.test.js --test --plugin @poi/puppet --framework mocha"
  },
  "dependencies": {
    "jump.js": "^1.0.2",
    "loadjs": "^4.3.0",
    "marked": "^14.1.2",
    "medium-zoom": "^1.1.0",
    "prismjs": "^1.29.0",
    "throttle-debounce": "^5.0.2",
    "vue": "^3.5.5",
    "vue-content-loader": "^2.0.1",
    "vue-router": "^4.4.5",
    "vue-router-prefetch": "^1.6.3",
    "vue-template-compiler": "^2.7.16",
    "vuex": "^4.1.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "@poi/plugin-puppet": "^0.1.6",
    "@semantic-release/git": "^10.0.1",
    "babel-preset-minimal": "^0.1.3",
    "babel-preset-power-assert": "^3.0.0",
    "bili": "^5.0.5",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "docute-google-analytics": "^1.1.0",
    "eslint-config-rem": "^4.0.0",
    "eslint-plugin-vue": "^9.28.0",
    "fast-async": "^6.3.8",
    "html-template-tag": "^4.1.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "poi": "^12.10.3",
    "postcss-import": "^16.1.0",
    "postcss-preset-env": "^10.0.3",
    "power-assert": "^1.6.1",
    "rollup-plugin-vue": "^6.0.0",
    "semantic-release": "^24.1.1",
    "vue-compile": "^1.1.2",
    "webpack-node-externals": "^3.0.0",
    "xo": "^0.59.3"
  },
  "repository": {
    "url": "egoist/docute",
    "type": "git"
  },
  "main": "dist/docute.js",
  "module": "lib/index.js",
  "files": [
    "dist",
    "lib",
    "!**/__test__/**"
  ],
  "description": "Effortlessly documentation done right.",
  "author": "egoist <0x142857@gmail.com>",
  "license": "MIT",
  "xo": {
    "extends": [
      "rem",
      "plugin:vue/essential"
    ],
    "prettier": true,
    "ignores": [
      "**/website/**",
      "**/dist/**"
    ],
    "envs": [
      "browser",
      "mocha"
    ],
    "globals": [
      "__DOCUTE_VERSION__"
    ],
    "extensions": [
      "vue"
    ],
    "rules": {
      "unicorn/filename-case": "off",
      "unicorn/no-abusive-eslint-disable": "off",
      "require-atomic-updates": "off"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md,vue}": [
      "xo --fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
